[DEFAULT]
devices = <%= devices %>
bind_ip = <%= storage_local_net_ip %>
bind_port = <%= bind_port %>
mount_check = <%= mount_check %>
user = <%= user %>
log_facility = LOG_LOCAL1
log_level = <%= log_level %>
workers = <%= workers %>

[pipeline:main]
pipeline = <%= pipeline.to_a.join(' ') %>

[app:account-server]
use = egg:swift#account

[account-replicator]
concurrency = <%= replicator_concurrency %>

[account-auditor]

[account-reaper]
concurrency = <%= reaper_concurrency %>
